{
  "name": "@travi/scaffolder-sub-command",
  "description": "scaffolder sub-command for commander",
  "version": "0.0.0-semantically-released",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "license": "MIT",
  "repository": "travi/scaffolder-sub-command",
  "bugs": "https://github.com/travi/scaffolder-sub-command/issues",
  "homepage": "https://npm.im/@travi/scaffolder-sub-command",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "clean": "rimraf lib/",
    "lint:js": "eslint . --cache",
    "lint:travis": "travis-lint .travis.yml",
    "lint:lockfile": "lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:*",
    "test:unit:base": "mocha --recursive test/unit",
    "test:unit": "nyc run-s test:unit:base",
    "test:integration": "run-s 'test:integration:base -- --tags \"not @wip\"'",
    "test:integration:base": "cucumber-js test/integration --require-module @babel/register --format-options '{\"snippetInterface\": \"async-await\"}'",
    "test:integration:debug": "DEBUG=test run-s test:integration",
    "test:integration:wip": "run-s 'test:integration:base -- --tags @wip'",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "run-s clean build:*",
    "build:js": "rollup -c",
    "watch": "run-s 'build:js -- --watch'",
    "prepublishOnly": "run-s build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/register": "7.10.1",
    "@travi/any": "2.0.9",
    "@travi/babel-preset": "3.0.22",
    "@travi/eslint-config": "1.0.35",
    "@travi/eslint-config-cucumber": "1.0.5",
    "@travi/eslint-config-mocha": "1.0.8",
    "@travi/language-scaffolder-prompts": "1.1.1",
    "@travi/travis-lint": "1.0.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "codecov": "3.7.0",
    "commitlint-config-travi": "1.3.6",
    "cucumber": "6.0.5",
    "cz-conventional-changelog": "3.2.0",
    "debug": "4.1.1",
    "http-status-codes": "2.1.2",
    "husky": "4.2.5",
    "lockfile-lint": "4.3.6",
    "mocha": "7.2.0",
    "mock-fs": "4.12.0",
    "mz": "2.7.0",
    "nock": "12.0.3",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "rollup": "2.13.1",
    "rollup-plugin-auto-external": "2.0.0",
    "sinon": "9.0.2",
    "testdouble": "3.16.0"
  },
  "dependencies": {
    "@form8ion/dependabot-scaffolder": "1.2.0",
    "@form8ion/github-actions-node-ci": "1.3.0",
    "@form8ion/hapi-scaffolder": "2.0.0",
    "@form8ion/react-components-scaffolder": "2.1.3",
    "@form8ion/renovate-scaffolder": "1.3.0",
    "@form8ion/ruby-scaffolder": "1.0.3",
    "@travi/circle-scaffolder-javascript": "1.2.1",
    "@travi/github-scaffolder": "5.1.1",
    "@travi/gitlab-scaffolder": "1.0.4",
    "@travi/javascript-scaffolder": "10.22.2",
    "@travi/netlify-scaffolder": "1.4.0",
    "@travi/node-app-engine-standard-scaffolder": "1.2.4",
    "@travi/project-scaffolder": "8.10.3",
    "@travi/shell-scaffolder": "1.1.1",
    "@travi/spectacle-scaffolder": "2.5.3",
    "@travi/travis-scaffolder-javascript": "3.2.1",
    "@travi/travis-scaffolder-shell": "1.0.0-alpha.4",
    "snyk": "^1.316.1"
  },
  "snyk": true
}
